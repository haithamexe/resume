{
  "version": 2,
  "builds": [
    {
      "src": "resume.pdf",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/Haitham Abdullah-Resume.pdf",
      "headers": {
        "Content-Type": "application/pdf",
        "Content-Disposition": "inline"
      }
    }
  ]
}
